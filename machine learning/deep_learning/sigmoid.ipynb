   "source": [
    "import numpy as np\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1.0 / (1 + np.exp(-x))\n",
    "\n",
    "inputs = np.array([0.7, -0.3])\n",
    "weights = np.array([0.1, 0.8])\n",
    "bias = -0.1\n",
    "\n",
    "# Calculate the output\n",
    "x = np.matmul(weights, inputs) + bias\n",
    "print(x)\n",
    "output = sigmoid(x)\n",
    "\n",
    "print('Output:')\n",
    "print(output)\n"
   ]
